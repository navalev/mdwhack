{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "southridge-mdw-adf"
		},
		"AzureDataLakeStorageGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageGen2'"
		},
		"FourthCoffeeFS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FourthCoffeeFS'"
		},
		"MoviesDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MoviesDB'"
		},
		"SalesDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SalesDB'"
		},
		"StreamingDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'StreamingDB'"
		},
		"VanArsdelSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'VanArsdelSqlServer'"
		},
		"AzureDataLakeStorageGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mdwoh03datalake.dfs.core.windows.net"
		},
		"FourthCoffeeFS_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "c:\\Rentals"
		},
		"FourthCoffeeFS_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "openhackadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "southridge-mdw-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageGen2_accountKey": {
						"value": "[parameters('AzureDataLakeStorageGen2_accountKey')]"
					},
					"MoviesDB_connectionString": {
						"value": "[parameters('MoviesDB_connectionString')]"
					},
					"SalesDB_connectionString": {
						"value": "[parameters('SalesDB_connectionString')]"
					},
					"StreamingDB_connectionString": {
						"value": "[parameters('StreamingDB_connectionString')]"
					},
					"FourthCoffeeFS_password": {
						"value": "[parameters('FourthCoffeeFS_password')]"
					},
					"VanArsdelSqlServer_connectionString": {
						"value": "[parameters('VanArsdelSqlServer_connectionString')]"
					},
					"AzureDataLakeStorageGen2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageGen2_properties_typeProperties_url')]"
					},
					"FourthCoffeeFS_properties_typeProperties_host": {
						"value": "[parameters('FourthCoffeeFS_properties_typeProperties_host')]"
					},
					"FourthCoffeeFS_properties_typeProperties_userId": {
						"value": "[parameters('FourthCoffeeFS_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "southridge-mdw-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'southridge-mdw-adf_ArmTemplate_0')]"
			]
		}
	]
}